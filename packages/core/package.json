{
  "name": "@pkg-placeholder/core",
  "type": "module",
  "version": "0.0.0",
  "description": "Core for pkg-placeholder",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/pkg-placeholder.git"
  },
  "bugs": "https://github.com/byronogis/pkg-placeholder/issues",
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "c12": "catalog:",
    "defu": "catalog:",
    "hookable": "catalog:",
    "pathe": "catalog:",
    "type-fest": "catalog:"
  }
}
