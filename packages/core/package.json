{
  "name": "@pkg-placeholder/core",
  "type": "module",
  "version": "0.0.0",
  "description": "Core for pkg-placeholder",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/pkg-placeholder.git"
  },
  "bugs": "https://github.com/byronogis/pkg-placeholder/issues",
  "keywords": [],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "c12": "catalog:deps",
    "defu": "catalog:deps",
    "hookable": "catalog:deps",
    "pathe": "catalog:deps",
    "type-fest": "catalog:types"
  }
}
