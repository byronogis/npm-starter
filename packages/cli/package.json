{
  "name": "@pkg-placeholder/cli",
  "type": "module",
  "version": "0.0.0",
  "description": "CLI for pkg-placeholder",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/pkg-placeholder.git"
  },
  "bugs": "https://github.com/byronogis/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.mjs",
      "./cli": "./dist/cli.mjs",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "pkg-placeholder": "./bin/index.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@pkg-placeholder/core": "workspace:*",
    "@types/fs-extra": "catalog:types",
    "chokidar": "catalog:cli",
    "citty": "catalog:cli",
    "colorette": "catalog:cli",
    "consola": "catalog:cli",
    "fs-extra": "catalog:deps",
    "pathe": "catalog:deps",
    "perfect-debounce": "catalog:deps",
    "tinyglobby": "catalog:deps"
  },
  "devDependencies": {}
}
