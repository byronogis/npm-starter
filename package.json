{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.3",
  "description": "_description_",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/pkg-placeholder.git"
  },
  "bugs": "https://github.com/byronogis/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "playground:dev": "vite playground",
    "playground:build": "vite build playground",
    "lint": "eslint .",
    "prepublishOnly": "automd && nr build",
    "release": "bumpp && pnpm publish",
    "start": "jiti src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "automd": "automd README.md",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@types/node": "^22.10.5",
    "@unocss/reset": "^0.65.3",
    "@vitest/coverage-v8": "^2.1.8",
    "automd": "^0.3.12",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.3.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^3.2.0",
    "unocss": "^0.65.3",
    "vite": "^6.0.7",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
